  <system.webServer>
    <rewrite>
            <rules>
                <rule name="Redirect musesbooks.com or musespublishers.com to www.musesbooks.com" stopProcessing="true">
                    <match url="^category/(.*)" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="^(www\.)?musespublishers.com$" />
                    </conditions>
                    <action type="Redirect" url="https://www.musesbooks.com/{R:1}" redirectType="Permanent" />
                </rule>  
                <rule name="RedirectNonWwwToWww" stopProcessing="true">
                  <match url="(.*)" />
                  <conditions>
                    <add input="{HTTP_HOST}" pattern="^musesbooks.com$" />
                  </conditions>
                  <action type="Redirect" url="http://www.musesbooks.com/{R:0}" redirectType="Permanent" />
                </rule> 
            </rules>
        </rewrite>
        <httpProtocol>
            <redirectHeaders>
                <!-- This is to ensure that clients don't cache the 301 itself - this is dangerous because the 301 can't change when put in place once it is cached -->
                <add name="Cache-Control" value="no-cache"/>
            </redirectHeaders>
        </httpProtocol>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
  </system.webServer>
